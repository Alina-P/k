// Testing mutually recursive uses of this.method

class oddeven {
  var n;
  method oddeven(x) {
    n = x;
  }
  method even() {
    if (n == 0) {
      return 1;
    }
    else {
      n = n - 1;
      return this.odd();
    }
  }
  method odd() {
    if (n == 0) {
      return 0;
    }
    else {
      n = n - 1;
      return this.even();
    }
  }
}

class main {
  method main() {
    print((new oddeven(17)).odd(), "\n");
  }
}

// 1
