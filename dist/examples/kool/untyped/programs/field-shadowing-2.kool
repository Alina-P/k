// Testing local variable shadowing a field

class i {
  var f;
  method i(v) {
    f = v;
  }
  method get() { return f; }
}

class g {
  var o;
  method g(o) {
    this.o = o;
  }
  method d() {
    var f = 9;
    return (o.get());
  }
}

class main {
  method main() {
    var t = new i(1), y = new g(t);
    print(y.d(), "\n");
  }
}

