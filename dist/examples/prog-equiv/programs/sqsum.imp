program1:

var i;
i := symInt(i01);
while (i <= n) do (
  s := s + i * i;
  i := i + 1
)

program2:

var i, k;
i := symInt(i02);
k := symInt(k0);
while (i <= n) do (
  s := s + k;
  k := k + i + i + 1;
  i := i + 1
)

condition:

  symInt(k0) == symInt(i02) * symInt(i02)
  and
  symInt(i01) == symInt(i02)

observers: n, s;


