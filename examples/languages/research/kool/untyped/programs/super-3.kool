// Testing field shadowing and super.

class c1 {
  var x, y;
  method c1() {
    x=1;
    y=2;
  }
}

class c2 extends c1 {
  var y, z;
  method c2() {
    c1();
    y=20;
    z=super.y;
  }
}

class c3 extends c2 {
  var x, y, z, u, w;
  method c3() {
    c2();
    x=100;
    y=200;
    z=super.x;
    u=super.y;
    w=super.z;
  }
}

class main {
  method main() {
    var o = new c3();
    print(o.x, " ", o.y, " ", o.z, " ", o.u, " ", o.w, "\n");
  }
}

// 100 200 1 20 2
