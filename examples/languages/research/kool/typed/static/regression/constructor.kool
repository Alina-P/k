// Testing new, constructor, method invocation.

class c {
  var i:int, j:int;
  method c(x:int) {
    i = x;
    j = ++x;
  }
  method add(d:int) {
    i = i+d;
    j = j-d;
  }
  method print2() {
    print(i, " ", j, "\n");
  }
}

class main {
  var a:int, b:int, o:class c;
  method main(){
    a = b = 5;
    o = new c(a);
    o.print2();
    o.add(++b);
    o.print2();
  }
}


// 5 6
// 11 0
