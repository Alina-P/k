class main {

  method foo() {
    try {
      throw 5;
      print(17);      // should not be printed
    } catch(e:int) {
      throw e + 2;    // throws 7
    }
    throw 1;          // should not be reached
  }

  method main() {
    try
      foo();
    catch(e:int)
      print(e,"\n");  // should print 7
  }

}

// 7
